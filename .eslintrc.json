{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "hermes-eslint",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "plugins": ["ft-flow"],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:ft-flow/recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {}
}
